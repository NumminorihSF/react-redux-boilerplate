  "scripts": {
    "prepush": "node -r babel-register scripts/prepush",
    "install:production": "NODE_ENV=production npm install",
    "install:all": "NODE_ENV=staging npm install",
    "install:sonar": "NODE_ENV=staging node ./scripts/install-sonar",
    "presonar:report": "npm run install:all",
    "sonar:report": "sreporter",
    "lint": "npm run _lint",
    "lint:fix": "npm run _lint -- --fix",
    "prelint:ci": "npm run build:target:dll",
    "lint:ci": "npm run _lint",
    "lint:changed": "npm run _lint -- --cache",
    "lint:watch": "node scripts/lint-watch.js -s",
    "lint:watch:changed": "node scripts/lint-watch.js -- --cache",
    "_lint": "eslint -c ./.eslintrc.yml \"src/**/*.js\"",
    "gen:types": "cross-env NODE_PATH=./src node -r babel-register scripts/gen-types.js",
    "preflow:prepare": "npm install flow-typed",
    "flow:prepare": "flow-typed install",
    "preflow:ci": "npm run flow:prepare",
    "flow:ci": "flow",
    "flow": "flow",
    "flow:watch": "cross-env NODE_PATH=./src node -r babel-register scripts/flow-watch.js",
    "build": "npm run build:production",
    "build:production": "cross-env NODE_ENV=production npm run _build",
    "build:staging": "cross-env NODE_ENV=staging npm run _build",
    "build:dev-server": "cross-env NODE_ENV=dev-server npm run _build",
    "build:develop": "cross-env NODE_ENV=staging npm run _build",
    "build:target:server": "cross-env TARGET=server webpack --config conf/webpack.config.babel.js",
    "prebuild:target:client": "gulp",
    "build:target:client": "cross-env TARGET=client webpack --config conf/webpack.config.babel.js",
    "build:target:dll": "cross-env TARGET=dll webpack --config conf/webpack.config.babel.js",
    "pre_build": "npm run build:target:dll",
    "_build": "npm run build:target:client",
    "--no-server--post_build": "npm run build:target:server",
    "gen:mock:data": "cross-env NODE_PATH=./src node -r babel-register scripts/gen-mock-data.js",
    "mock:server": "cross-env NODE_PATH=./src node -r babel-register scripts/mock-server.js",
    "predev:server": "cross-env NODE_ENV=dev-server cross-env NODE_PATH=./src npm run build:target:dll",
    "dev:server": "cross-env NODE_ENV=dev-server cross-env NODE_PATH=./src node scripts/dev-server",
    "_dev:server": "cross-env TARGET=client cross-env NODE_ENV=dev-server cross-env NODE_PATH=./src node scripts/dev-server/webpack-server-executable.js",
    "dev:server:notest": "cross-env NODE_SKIP_TESTS=1 npm run dev:server",
    "dev:server:nolint": "cross-env NODE_SKIP_LINT=1 npm run dev:server",
    "dev:server:noflow": "cross-env NODE_SKIP_FLOW=1 npm run dev:server",
    "dev:server:fast": "cross-env NODE_SKIP_TESTS=1 cross-env NODE_SKIP_LINT=1 cross-env NODE_SKIP_FLOW=1 npm run dev:server",
    "dev:server:isolate": "cross-env NODE_SKIP_TESTS=1 cross-env NODE_SKIP_LINT=1 cross-env NODE_SKIP_FLOW=1 cross-env NODE_PATH=./src node -r babel-register scripts/dev-server-isolate",
    "start": "cross-env NODE_ENV=production cross-env NODE_PATH=./build node build/server",
    "test:watch": "npm run _test -- -w",
    "test": "npm run _test",
    "test:ci": "npm run _test",
    "_test": "cross-env API_BASE_URL=NO_BASE_URL cross-env NODE_ENV=test cross-env NODE_PATH=./src mocha --compilers js:babel-register -r test-support/setup.mocha.js --recursive \"src/**/*.test.js\""
  },
  "author": "Konstantine Petryaev <numminorihsf@gmail.com>",
