{
  "name": "my_app_boilerplate",
  "version": "0.9.0",
  "description": "boilerplate for react-redux app",
  "main": "index.js",
  "scripts": {
    "lint": "eslint -c ./.eslint-rc.yml \"src/**/*.js\"",
    "prebuild": "npm install --loglevel=warn",
    "build": "NODE_ENV=production npm run build:prod",
    "prebuild:prod": "npm run build:prod:client",
    "build:prod": "npm run build:prod:server",
    "build:prod:client": "webpack -p",
    "build:prod:server": "gulp",
    "prebuild:dev": "npm run build:dev:client",
    "build:dev": "npm run build:dev:server",
    "build:dev:client": "webpack",
    "build:dev:server": "gulp",
    "dev:server": "node webpack.server.js",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node build/server",
    "test:watch": "cross-env NODE_ENV=test cross-env NODE_PATH=./src mocha --compilers js:babel-register -r test/support/setup.mocha.js --recursive 'test/**/*.test.js' -w",
    "test:ci": "cross-env NODE_ENV=test cross-env NODE_PATH=./src mocha --compilers js:babel-register -r test/support/setup.mocha.js --recursive 'test/**/*.test.js'",
    "test": "npm run test:ci"
  },
  "author": "Konstantine Petryaev <numminorihsf@gmail.com>",
  "license": "MIT",
  "browsers": [
    "last 2 version",
    "ie >= 8",
    "Android >= 2",
    "iOS >= 7",
    "Safari >= 5"
  ],
  "buildDestination": "build",
  "dependencies": {
    "bluebird": "^3.4.6",
    "express": "^4.14.0",
    "humps": "^2.0.0",
    "immutable": "^3.8.1",
    "pug": "^2.0.0-beta6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "superagent": "^2.3.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "autopolyfiller-loader": "^1.0.0",
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "csso-loader": "^0.2.0",
    "enzyme": "^2.5.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^1.3.2",
    "html-webpack-plugin": "^2.24.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "nock": "^9.0.0",
    "postcss-loader": "^1.0.0",
    "react-addons-test-utils": "^15.3.2",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.0"
  }
}
