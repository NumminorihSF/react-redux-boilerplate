[ignore]
.*/npm/.*
.*/build/.*
.*/dest/.*
.*/stylelint/.*

[include]

[libs]

[options]
module.name_mapper.extension='css' -> '<PROJECT_ROOT>/flow-support/CSSFlowStub.js.flow'
module.name_mapper.extension='scss' -> '<PROJECT_ROOT>/flow-support/CSSFlowStub.js.flow'

module.name_mapper.extension='svg' -> '<PROJECT_ROOT>/flow-support/FileLoaderStub.js.flow'
